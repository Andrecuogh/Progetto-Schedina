#:include layouts/scorepage/utilbar/UtilBar.kv
#:include layouts/scorepage/navigation/PageNavigator.kv
#:include layouts/scorepage/resultbox/ResultBox.kv


<ScorePage>:
    utilbar: utilbar
    navigation: navigation
    resultbox: resultbox
    canvas:
        Color:
            rgba: app.colors["label"]
        Rectangle:
            pos: root.size[0] * 0.00, root.size[1] * 0.80
            size: root.size[0] * 1.00, root.size[1] * 0.2
    UtilBar:
        id: utilbar
    PageNavigator:
        id: navigation
    ResultBox:
        id: resultbox