#:include layouts/scorepage/probabilities/ProbabilityScreen.kv
#:include layouts/scorepage/accessories/AccessoriesScreen.kv

<ResultBox@ScrollView>:
    pos_hint: {"x": 0.00, "y": 0.00}
    size_hint: (1.00, 0.8)
    swipe_distance: 200*app.sf
    on_touch_down:
        self.pos_initial = args[1].pos
    on_touch_up:
        self.pos_final = args[1].pos
        app.change_view(self)
    ScreenManager:
        size_hint: (None, None)
        size: (root.size[0]*1.01, root.size[1]*1.01)
        ProbabilityScreen:
        AccessoriesScreen: